{
  "name": "frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PORT=4002 next dev",
    "build": "next build",
    "build:dev": "env-cmd -f .env.development yarn build",
    "build:prod": "env-cmd -f .env.production yarn build",
    "build:css": "yarn build:sass && npx tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css && yarn format:prettier-css",
    "build:tailwind": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css --watch",
    "build:sass-watch": "sass --no-source-map ./src/styles/scss:./src/styles/css --watch",
    "build:sass": "sass --no-source-map ./src/styles/scss:./src/styles/css",
    "start": "next start",
    "lint": "next lint --dir src",
    "format:eslint": "eslint --fix --ignore-path .eslintignore './**/*.{js,jsx,ts,tsx}' && yarn format:prettier-tsx",
    "format:prettier": "prettier --write --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css,scss, README.md}'",
    "format:prettier-css": "prettier --write --ignore-path .prettierignore './**/*.{css,scss}'",
    "format:prettier-tsx": "prettier --write --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx}'",
    "check:types": "tsc --pretty --noEmit",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint . --ext ts --ext tsx --ext js",
    "prepare": "husky install",
    "test": "jest",
    "release": "semantic-release --no-ci",
    "precommit": "git-format-staged --formatter 'prettier --stdin' './**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "check-types": "tsc --pretty --noEmit",
    "check-lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@ethersproject/providers": "^5.7.2",
    "@fontsource/roboto": "^4.5.8",
    "@mui/material": "^5.10.9",
    "@mui/styled-engine-sc": "^5.10.6",
    "@next/bundle-analyzer": "^13.0.2",
    "@tanstack/react-query": "^4.14.5",
    "axios": "^1.1.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.6",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "ethers": "^5.7.2",
    "jwt-decode": "^3.1.2",
    "next": "13.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.6",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "18.8.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^29.1.2",
    "eslint": "^8.25.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-testing-library": "5.5.0",
    "git-format-staged": "^3.0.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "node-mocks-http": "^1.11.0",
    "postcss": "^8.4.17",
    "postcss-cli": "^10.0.0",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "semantic-release": "^19.0.5",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "**/async": "^2.6.4",
    "**/node-fetch": "^2.6.7",
    "webpack": "^5"
  },
  "lint-staged": {
    "*.(tsx|ts)": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
